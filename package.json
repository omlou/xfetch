{
  "name": "@xlou/xfetch",
  "version": "1.0.3",
  "main": "./dist/es/xfetch.min.js",
  "module": "./dist/es/xfetch.min.js",
  "browser": "./dist/umd/xfetch.min.js",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./dist/umd/xfetch.min.js",
      "default": "./dist/es/xfetch.min.js"
    },
    "./es": {
      "types": "./index.d.ts",
      "default": "./dist/es/xfetch.min.js"
    },
    "./umd": {
      "types": "./index.d.ts",
      "default": "./dist/umd/xfetch.min.js"
    }
  },
  "type": "module",
  "types": "./index.d.ts",
  "keywords": [
    "fetch",
    "xhr",
    "XMLHttpRequest",
    "ajax"
  ],
  "description": "Use fetch/xhr more conveniently and quickly.",
  "repository": {
    "type": "git",
    "url": "https://github.com/omlou/xfetch.git"
  },
  "unpkg": "dist/umd/xfetch.min.js",
  "jsdelivr": "dist/umd/xfetch.min.js",
  "scripts": {
    "build": "gulp clear && rollup -c",
    "start": "gulp serve",
    "upgrade": "gulp upgrade --host %npm_config_host%",
    "serve": "node app.js"
  },
  "author": "Louis Xu",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.1",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-webserver": "^0.9.1",
    "multer": "^1.4.5-lts.1",
    "minimist": "^1.2.8",
    "rollup": "^3.24.0",
    "rollup-plugin-dts": "^5.3.0",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5"
  }
}
